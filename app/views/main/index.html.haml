                              <div class="hr large-1 small-3"></div>
                              </div>

        .l-submain
          .l-submain-h.g-html
            .page-header.pad-bottom.text-center
              %h1{style: "text-align: center; letter-spacing: 5px; word-spacing: 5px; font-size: 200%; color: #ccc;"} "WE KNOW A LOT ABOUT A LOT"
              .hr.light.large-1.small-3
            .g-cols.intro-section{style: "margin-top: 40px;"}
              .one-fourth
                %h6 FINANCE
                %p We are financial wizards.
                = image_tag("/img/icons/V2/64/Keywords & Rankings.png", class: "center")
                /=link_to image_tag("/img/icons/V2/64/Keywords & Rankings.png", class: "center"), creative_path
              .one-fourth
                %h6 CREATIVE
                %p We design stuff.
                =image_tag("/img/icons/V2/64/Creative Campaigns.png", class: "center")
                /=link_to image_tag("/img/icons/V2/64/Creative Campaigns.png", class: "center"), creative_path
              .one-fourth
                %h6 TECHNICAL
                %p We build Rails apps.
                =image_tag("/img/icons/V2/64/Responsive Design.png", class: "center")
                /=link_to image_tag("/img/icons/V2/64/Responsive Design.png", class: "center"), technical_path
              .one-fourth
                %h6 STRATEGY
                %p We help companies grow.
                =image_tag("/img/icons/V2/64/SEO Training.png", class: "center")
                /=link_to image_tag("/img/icons/V2/64/SEO Training.png", class: "center"), strategy_path
