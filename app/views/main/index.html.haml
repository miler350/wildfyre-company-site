                    %ul
                      %li{"data-slotamount" => "7", "data-transition" => "fade"}
                        %img{src: "/img/wf_head.jpg", alt: ""}
                        .caption.sfb{"data-easing" => "easeInOutSine", "data-speed" => "400", "data-start" => "400", "data-x" => "-70", "data-y"=> "90"}
                          %h1.wf-logo WILDFYRE
                          .caption.sfb{"data-easing" => "easeInOutSine", "data-speed" => "400", "data-start" => "400", "data-x" => "200", "data-y" => "290"}
                            /
                              <button class="g-btn type_secondary size_big" href="#">
                              <span>
                              JOIN THE WILDFYRE NETWORK
                              <i class="icon-chevron-sign-right"></i>
                              </span>
                              </button>
                              
                              <!-- <div class="tile-wrapper">
