			<%= form_for [@user, @contact] do |f| %>
              <fieldset>
                <div class="form-group">
                  <h5>Name and Title.</h5>
                    <div class="row">
                      <div class="col-lg-4">
			
			
              <label for="firstName"><%= f.label :contact_first_name, 'First' %></label>
			<%= f.text_field :contact_first_name, :autofocus => true, class: "col-lg-12 form-inline form-control", placeholder: "First name" %>
		   </div>
		   
		   
              <div class="col-lg-4">
              <label for="middleName"><%= f.label :contact_middle_name, 'Middle' %></label>
			<%= f.text_field :contact_middle_name, class: "col-lg-12 form-inline form-control", placeholder: "Middle name" %>
		   </div>
		   
           
          <div class="col-lg-4">
              <label for="lastName"><%= f.label :contact_last_name, 'Last' %></label>
			<%= f.text_field :contact_last_name, class: "col-lg-12 form-inline form-control", placeholder: "Last name" %>
		</div>
        </div>
    </div>
  
  
  
    <div class="form-group">
        <div class="row">
          <div class="col-lg-6">
              <label for="company"><%= f.label :contact_company, 'Company' %></label>
			<%= f.text_field :contact_company, class: "col-lg-12 form-inline form-control", placeholder: "Company" %>
			
              
          </div>
          <div class="col-lg-6">
              <label for="title"><%= f.label :contact_title, 'Title' %></label>
			<%= f.text_field :contact_title, class: "col-lg-12 form-inline form-control", placeholder: "Title" %>				
          </div>
        </div>
    </div>
    
    
    <h5>Email, Phone, Web.</h5>
    <div class="form-group">
        <div class="row">
          <div class="col-lg-6">
              <label for="personalEmail"><%= f.label :contact_personal_email, 'Personal Email' %></label>
			<%= f.text_field :contact_personal_email, class: "col-lg-12 form-inline form-control", placeholder: "Personal Email" %>
          </div>
		
		
          <div class="col-lg-6">
              <label for="workEmail"><%= f.label :contact_work_email, 'Work Email' %></label>
              <%= f.text_field :contact_personal_email, class: "col-lg-12 form-inline form-control", placeholder: "Work Email" %>
          </div>
        </div>
    </div>
  
  
    <div class="form-group">
        <div class="row">
          <div class="col-lg-6">
              <label for="cellPhone"><%= f.label :contact_cell_phone, 'Mobile/Cell' %></label>
             <%= f.text_field :contact_cell_phone, class: "col-lg-12 form-inline form-control", placeholder: "Cell Phone" %>
          </div>
		
		
          <div class="col-lg-6">
              <label for="workPhone"><%= f.label :contact_work_phone, 'Work' %></label>
              <%= f.text_field :contact_work_phone, class: "col-lg-12 form-inline form-control", placeholder: "Work Phone" %>
          </div>
        </div>
    </div>
    <div class="form-group">
          <label for="website"><%= f.label :contact_wesbite, 'Website (URL)' %></label>
		<%= f.text_field :contact_website, class: "col-lg-12 form-inline form-control", placeholder: "paste in your website URL" %>
    </div>

    <h5>Address.</h5>
    <div class="form-group">
        <div class="row">
          <div class="col-lg-2">
			
			
              <label for="country"><%= f.label :contact_country, 'Country' %></label>
             <%= f.text_field :contact_country, class: "col-lg-12 form-inline form-control", placeholder: "Country" %>
          </div>
		
		
          <div class="col-lg-6">
              <label for="projectDescription"><%= f.label :contact_street_address, 'Country' %></label>
               <%= f.text_field :contact_street_address, class: "col-lg-12 form-inline form-control", placeholder: "Enter Address" %>
          </div>
		
		
          <div class="col-lg-4">
              <label for="projectDescription"><%= f.label :contact_city, 'City' %></label>
              <%= f.text_field :contact_city, class: "col-lg-12 form-inline form-control", placeholder: "City" %>
          </div>
        </div>
    </div>
  
  
    <div class="form-group">
        <div class="row">
            <div class="col-lg-6">
                <label for="projectDescription"><%= f.label :contact_state, 'State' %></label>
                <%= f.text_field :contact_state, class: "col-lg-12 form-inline form-control", placeholder: "State" %>
            </div>
		  
		  
            <div class="col-lg-4">
                <label for="projectDescription"><%= f.label :contact_zip, 'Zip' %></label>
			   <%= f.text_field :contact_zip, class: "col-lg-12 form-inline form-control", placeholder: "Zip" %>
            </div>
        </div>
    </div>
  
  
    <h5>Social Media.</h5>
        <div class="form-group">
            <label for="projectDescription"><%= f.label :contact_facebook, 'Facebook' %></label>
		  <%= f.text_field :contact_facebook, class: "form-control", placeholder: "paste Facebook profile URL link" %>
        </div>
	  
	  
        <div class="form-group">
           <label for="projectDescription"><%= f.label :contact_linkedin, 'LinkedIn' %></label>
           <%= f.text_field :contact_linkedin, class: "form-control", placeholder: "paste LinkedIn profile URL link" %>
        </div>
	  
	  
        <div class="form-group">
            <label for="projectDescription"><%= f.label :contact_twitter, 'Twitter' %></label>
            <%= f.text_field :contact_twitter, class: "form-control", placeholder: "paste Twitter profile URL link" %>
        </div>
	  
	  
    <h5>Education.</h5>
    <div class="form-group">
        <div class="row">
            <div class="col-lg-6">
                <label for="projectDescription"><%= f.label :contact_undergrad, 'Undergrad School' %></label>
                <%= f.text_field :contact_undergrad, class: "col-lg-12 form-inline form-control", placeholder: "Undergrad School" %>
            </div>
		  
		  
            <div class="col-lg-6">
                <label for="projectDescription"><%= f.label :contact_undergrad_degree, 'Degree' %></label>
                <%= f.text_field :contact_undergrad_degree, class: "col-lg-12 form-inline form-control", placeholder: "Undergrad Degree" %>
            </div>
        </div>
    </div>
  
  
    <div class="form-group">
        <div class="row">
              <div class="col-lg-6">
                  <label for="projectDescription"><%= f.label :contact_grad, 'Grad School' %></label>
                  <%= f.text_field :contact_grad, class: "col-lg-12 form-inline form-control", placeholder: "Grad School" %>
              </div>
			
			
              <div class="col-lg-6">
                  <label for="projectDescription"><%= f.label :contact_grad_degree, 'Degree' %></label>
                   <%= f.text_field :contact_grad_degree, class: "col-lg-12 form-inline form-control", placeholder: "Grad Degree" %>
              </div>
        </div>
    </div>
    
    <h5>Birthday and Personal.</h5>
    <div class="form-group">
        <div class="row">
		  
		  
              <div class="col-lg-4">
                  <label for="projectDescription"><%= f.label :contact_birthday, 'Birthday' %></label>
                  <%= f.date_field :contact_birthday, class: "col-lg-12 form-inline form-control", placeholder: "when?" %>
              </div>
			
			
              <div class="col-lg-4">
                  <label for="projectDescription"><%= f.label :contact_married, 'Married?' %></label>
                   <%= f.text_field :contact_married, class: "col-lg-12 form-inline form-control", placeholder: "how long?" %>
              </div>
			
			
              <div class="col-lg-4">
                  <label for="projectDescription"><%= f.label :contact_kids, 'Kids?' %></label>
                  <%= f.text_field :contact_kids, class: "col-lg-12 form-inline form-control", placeholder: "how many?" %>
              </div>
          </div>
    </div>


    <h5>Notes.</h5>
    <div class="form-group">
      <label for="projectDescription">Comments/Notes</label>
      <textarea class="form-control" rows="3" placeholder="Anything else to know..."></textarea>
    </div>

    <h5>Upload Photo.</h5>
    <div class="form-group">
      <label for="userPhoto">Project Files</label>
      <input type="file" id="userPhoto">
    </div>
    <div class="checkbox">
      <label>
        <input type="checkbox"> I agree to Wildfyre terms of service.
      </label>
</div>
  <br/>
<%= f.submit "ADD NEW CONTACT", :class => "btn btn-default"  %>
  <% end %>